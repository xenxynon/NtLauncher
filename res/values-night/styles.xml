<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AddItemActivityTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="widgetsTheme">@style/WidgetContainerTheme.Dark</item>
    </style>
    <style name="AllSetTheme" parent="@android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:enforceNavigationBarContrast">false</item>
    </style>
    <style name="GestureTutorialActivity" parent="@style/AppTheme">
        <item name="background">@android:color/transparent</item>
        <item name="onSurfaceBack">@color/materialColorTertiaryFixedDim</item>
        <item name="onSurfaceHome">@color/materialColorPrimaryFixedDim</item>
        <item name="onSurfaceOverview">@color/materialColorPrimaryFixed</item>
        <item name="secondaryBack">@color/materialColorOnTertiaryFixed</item>
        <item name="secondaryHome">@color/materialColorOnPrimaryFixed</item>
        <item name="secondaryOverview">@color/materialColorOnSecondaryFixed</item>
        <item name="surfaceBack">@color/materialColorOnTertiaryFixedVariant</item>
        <item name="surfaceContainer">@color/materialColorSurfaceContainer</item>
        <item name="surfaceHome">@color/materialColorOnPrimaryFixedVariant</item>
        <item name="surfaceOverview">@color/materialColorOnSecondaryFixedVariant</item>
        <item name="tutorialSubtitle">@android:color/white</item>
    </style>
    <style name="NTHomeSettings" parent="@style/HomeSettings.Theme">
        <item name="android:colorBackground">@color/default_color_background_dark</item>
        <item name="android:scrollbars">none</item>
    </style>
    <style name="ReachabilityEduHandLayout" parent="@style/Theme.AppCompat">
        <item name="android:textAppearance">@android:style/PreferencePanel</item>
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:lineSpacingExtra">-1.0sp</item>
        <item name="android:textAlignment">center</item>
    </style>
    <style name="TextAppearance.GestureTutorial.ButtonLabel.Back" parent="@style/TextAppearance.GestureTutorial.ButtonLabel">
        <item name="android:textColor">?secondaryBack</item>
    </style>
    <style name="TextAppearance.GestureTutorial.ButtonLabel.Home" parent="@style/TextAppearance.GestureTutorial.ButtonLabel">
        <item name="android:textColor">?secondaryHome</item>
    </style>
    <style name="TextAppearance.GestureTutorial.ButtonLabel.Overview" parent="@style/TextAppearance.GestureTutorial.ButtonLabel">
        <item name="android:textColor">?secondaryOverview</item>
    </style>
    <style name="TextAppearance.GestureTutorial.MainTitle.Success.Back" parent="@style/TextAppearance.GestureTutorial.MainTitle.Back" />
